    <section name="rebus" type="Rebus.Configuration.RebusConfigurationSection, Rebus" />
  </configSections>

  <rebus inputQueue="my-app.input" errorQueue="my-app.error" workers="1">
    <endpoints>
      <!-- <add messages="SomeAssembly" endpoint="another-app.input"/> -->
